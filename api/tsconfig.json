{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "..",
    "module": "commonjs",
    "target": "ES2020",
    "lib": ["ES2020"],
    "noEmit": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}
